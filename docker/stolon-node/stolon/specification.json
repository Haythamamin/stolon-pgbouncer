{
	"initMode": "new",
  "sleepInterval": "5s",
  "requestTimeout": "10s",
  "failInterval": "20s",
  "deadKeeperRemovalInterval": "48h",
  "maxStandbys": 20,
  "maxStandbysPerSender": 3,
  "maxStandbyLag": 1048576,
  "synchronousReplication": true,
  "minSynchronousStandbys": 1,
  "maxSynchronousStandbys": 1,
  "additionalWalSenders": 5,
  "additionalMasterReplicationSlots": [

  ],
  "mergePgParameters": true,
  "clusterRole": "master",
  "automaticPgRestart": false,
  "usePgrewind": true,
  "pgParameters": {
    "archive_command": "/bin/true",
    "archive_mode": "true",
    "autovacuum": "true",
    "checkpoint_completion_target": "0.5",
    "checkpoint_timeout": "60s",
    "checkpoint_warning": "60s",
    "datestyle": "iso, mdy",
    "lc_messages": "C",
    "log_checkpoints": "true",
    "log_connections": "true",
    "log_directory": "/var/log/postgresql",
    "log_disconnections": "true",
    "log_filename": "postgresql.log",
    "log_line_prefix": "%t [%p]: [%l-1] user=%u,db=%d,app=%a ",
    "log_lock_waits": "true",
    "log_min_duration_statement": "500",
    "log_temp_files": "0",
    "logging_collector": "true",
    "wal_buffers": "8MB",
    "wal_level": "hot_standby"
  },
  "pgHBA": [
    "local\tall\tall\t\tpeer",
    "host\tall\tall\t127.0.0.1/32\ttrust",
    "host\tall\tall\t::1/128\ttrust",
    "host\tall\tall\t10.0.0.0/8\ttrust",
    "hostssl\tall\tall\t10.0.0.0/8\ttrust",
    "host\treplication\tall\t10.0.0.0/8\ttrust",
    "hostssl\treplication\tall\t10.0.0.0/8\ttrust",
    "host\tall\tall\t172.16.0.0/12\ttrust",
    "hostssl\tall\tall\t172.16.0.0/12\ttrust",
    "host\treplication\tall\t172.16.0.0/12\ttrust",
    "hostssl\treplication\tall\t172.16.0.0/12\ttrust"
  ]
}
